-- Script generated by Meta Llama 4
-- Services
local Players = game:GetService("Players")
local ReplicatedStorage = game:GetService("ReplicatedStorage")
-- RemoteEvents
local SpawnPetEvent = Instance.new("RemoteEvent")
SpawnPetEvent.Name = "SpawnPet"
SpawnPetEvent.Parent = ReplicatedStorage
local SpawnSeedEvent = Instance.new("RemoteEvent")
SpawnSeedEvent.Name = "SpawnSeed"
SpawnSeedEvent.Parent = ReplicatedStorage
-- Player data
local playerData = {}
-- Function to spawn a pet
local function spawnPet(player)
    -- Define pet names
    local pets = {"Raccoon", "Red Fox", "Disco Bee", "Queen Bee", "Butterfly", "Mimic Octopus", "Scarlet Macaw", "Dragonfly"}

    -- Spawn a random pet
    local petName = pets[math.random(1, #pets)]

    -- Store the pet data
    if not playerData[player.UserId] then
        playerData[player.UserId] = {pets = {}}
    end
    table.insert(playerData[player.UserId].pets, petName)

    -- Fire the RemoteEvent to notify clients to render the pet
    SpawnPetEvent:FireClient(player, petName)
end
-- Function to spawn a seed
local function spawnSeed(player)
    -- Define seed names
    local seeds = {"Candy Blossom", "Elephant Ears", "Sunflower", "Moon Blossom", "Moon Mango", "Moon Melon"}

    -- Spawn a random seed
    local seedName = seeds[math.random(1, #seeds)]

    -- Store the seed data
    if not playerData[player.UserId] then
        playerData[player.UserId] = {seeds = {}}
    end
    table.insert(playerData[player.UserId].seeds, seedName)

    -- Fire the RemoteEvent to notify clients to render the seed
    SpawnSeedEvent:FireClient(player, seedName)
end
-- Auto-collect feature
local function autoCollect(player)
    -- Define the collectibles
    local collectibles = {
        "Carrot",
        "Strawberry",
        "Blueberry",
        "Tomato",
        "Banana",
        "Kiwi",
        "Loquat",
        "Sugar",
        "Apple",
        "Green Apple",
        "Pear",
        "Cantaloupe",
        "Cauliflower",
        "Avocado"
    }

    -- Loop through all collectibles in the workspace
    for _, collectible in pairs(workspace:GetDescendants()) do
        -- Check if the collectible is in the list
        if collectible:IsA("BasePart") and table.find(collectibles, collectible.Name) then
            -- Check if the player is nearby
            local distance = (player.Character.HumanoidRootPart.Position - collectible.Position).Magnitude
            if distance <= 5 then
                -- Collect the item
                collectible.Parent = nil

                -- Give the player the collected item
                if not playerData[player.UserId] then
                    playerData[player.UserId] = {collectibles = {}}
                end
                table.insert(playerData[player.UserId].collectibles, collectible.Name)
            end
        end
    end
end
-- Connect to the RemoteEvents
SpawnPetEvent.OnServerEvent:Connect(function(player)
    spawnPet(player)
end)
SpawnSeedEvent.OnServerEvent:Connect(function(player)
    spawnSeed(player)
end)
-- Run the auto-collect function every second
while true do
    for _, player in pairs(Players:GetPlayers()) do
        autoCollect(player)
    end
    wait(1)
end
